<!ELEMENT reservoirInfo ((regions, rivers, reservoirs))>
<!ELEMENT regions ((region+))>
<!ELEMENT region EMPTY>
<!ATTLIST region
		 id ID #REQUIRED
		 name CDATA #REQUIRED>
<!ELEMENT rivers ((river+))>
<!ELEMENT river EMPTY>
<!ATTLIST river
		 id ID #REQUIRED
		 name CDATA #REQUIRED>
<!ELEMENT reservoirs ((reservoir+))>
<!ELEMENT reservoir (name, depth, damHeight, length, coordinates, area, fish)>
<!ATTLIST reservoir 
		 id ID #REQUIRED
		 regionId IDREF #REQUIRED
		 riverId IDREF #REQUIRED
		 photo ENTITIES #IMPLIED>
<!ELEMENT name (#PCDATA)>
<!ELEMENT depth (#PCDATA)>
<!ELEMENT damHeight (#PCDATA)>
<!ELEMENT length (#PCDATA)>
<!ELEMENT coordinates (latitude, longitude)>
<!ELEMENT latitude (#PCDATA)>
<!ELEMENT longitude (#PCDATA)>
<!ELEMENT area (#PCDATA)>
<!ELEMENT fish (#PCDATA)>
